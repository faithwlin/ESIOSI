<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.esiosi">
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:exported="true"/>
        <activity android:name=".Progress" />
        <activity android:name=".Profile" />
        <activity android:name=".Quiz" />
        <activity android:name=".Flashcards" />
        <activity android:name=".OSILayers" />
        <activity android:name=".LessonPhysical" />
        <activity android:name=".LessonDatalink" />
        <activity android:name=".LessonNetwork" />
        <activity android:name=".LessonTransport" />
        <activity android:name=".LessonSession" />
        <activity android:name=".LessonPresentation" />
        <activity android:name=".LessonApplication" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>